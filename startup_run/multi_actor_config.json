{
  "api_key": {
    "deepseek": "your_api_key_here",
    "qwen": "your_api_key_here",
    "zhipu": "your_api_key_here"
  },
  "llm": {
    "use": "qwen",
    "model_name": "qwen-turbo",
    "context_window": 120000,
    "max_token": 8000,
    "top_p": 0.9,
    "temperature": 0.75,
    "time_out": 300.0
  },
  "text_embed": {
    "embed_model_name": "BAAI/bge-large-en-v1.5"
  },
  "dataset": {
    "data_source": "bird:dev:",
    "data_source_dir": "../files/data_source",
    "need_few_shot": false,
    "need_external": false
  },
  "database": {
    "skip_schema_init": false,
    "schema_source": "bird:dev",
    "multi_database": false,
    "vector_store": "../vector_store",
    "schema_source_dir": "../files/schema_source",
    "need_build_index": false
  },
  "task": {
    "task_meta": [
      {
        "task_id": "parse1",
        "task_type": "ParseTask",
        "data_source": "bird:dev:",
        "schema_source": "bird:dev",
        "is_save_dataset": true,
        "meta": {
          "task": {
            "parse_type": "DINSQLCoTParser"
          }
        }
      },
      {
        "task_id": "parse2",
        "task_type": "ParseTask",
        "data_source": "bird:dev:",
        "schema_source": "bird:dev",
        "is_save_dataset": true,
        "meta": {
          "task": {
            "parse_type": "LinkAlignParser"
          }
        }
      },
      {
        "task_id": "scale",
        "task_type": "ScaleTask",
        "data_source": "bird:dev:",
        "schema_source": "bird:dev",
        "is_save_dataset": true,
        "meta": {
          "task": {
            "scale_type": "ChessScaler"
          }
        },
        "open_parallel": true,
        "max_workers": 1
      },
      {
        "task_id": "optimize",
        "task_type": "OptimizeTask",
        "data_source": "bird:dev:",
        "schema_source": "bird:dev",
        "is_save_dataset": true,
        "meta": {
          "task": {
            "optimize_type": "LinkAlignOptimizer"
          }
        },
        "open_parallel": true,
        "max_workers": 1
      },
      {
        "task_id": "select",
        "task_type": "SelectTask",
        "data_source": "bird:dev:",
        "schema_source": "bird:dev",
        "is_save_dataset": true,
        "meta": {
          "task": {
            "select_type": "CHESSSelector"
          }
        },
        "open_parallel": true,
        "max_workers": 1
      }
    ],
    "cpx_task_meta": [
      {
        "task_id": "complex_actor",
        "task_lis": [
          [
            "parse1",
            "parse2"
          ],
          "scale",
          "optimize",
          "select"
        ],
        "eval_type": [
          "execute_accuracy"
        ],
        "is_save_dataset": true,
        "dataset_save_path": "../files/datasets/bird_dev_pipeline.json",
        "meta": {
          "actor": {
            "parse1": {
              "save_dir": "../files/schema_links/bird_dev"
            },
            "parse2": {
              "save_dir": "../files/schema_links/bird_dev"
            },
            "scale": {
              "save_dir": "../files/pred_sql/bird_dev"
            },
            "optimize": {
              "save_dir": "../files/pred_sql/bird_dev"
            },
            "select": {
              "save_dir": "../files/pred_sql/bird_dev"
            }
          }
        },
        "open_parallel": true,
        "max_workers": 5
      }
    ],
    "is_save_dataset": true
  },
  "engine": {
    "exec_process": [
      "complex_actor"
    ]
  }
}