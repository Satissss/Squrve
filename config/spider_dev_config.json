{
  "api_key": {
    "qwen": "your_qwen_api_key_here",
    "deepseek": "your_deepseek_api_key_here",
    "zhipu": "your_zhipu_api_key_here"
  },
  "llm": {
    "use": "qwen",
    "model_name": "qwen-turbo",
    "context_window": 120000,
    "max_token": 8000,
    "top_p": 0.9,
    "temperature": 0.75,
    "time_out": 300.0
  },
  "text_embed": {
    "embed_model_source": "HuggingFace",
    "embed_model_name": "BAAI/bge-large-en-v1.5"
  },
  "router": {
    "use_demo": false
  },
  "dataloader": {
    "is_prepare_data": true
  },
  "dataset": {
    "data_source": "spider:dev",
    "data_source_dir": "files/data_source",
    "overwrite_exist_file": true,
    "default_data_file_name": "spider_dev_data.json",
    "need_few_shot": false,
    "few_shot_num": 3,
    "few_shot_save_dir": "files/reasoning_examples/user",
    "few_shot_range": [],
    "need_external": false,
    "external_range": [],
    "external_save_dir": "files/external",
    "db_path": "benchmarks/spider/database"
  },
  "database": {
    "skip_schema_init": false,
    "schema_source": "spider:dev",
    "multi_database": false,
    "vector_store": "vector_store",
    "schema_source_dir": "files/schema_source",
    "default_schema_dir_name": "spider_dev_schema",
    "need_build_index": false,
    "index_range": false
  },
  "reducer": {
    "reduce_type": "LinkAlign",
    "is_save_reduce": true,
    "reduce_save_dir": "files/instance_schemas",
    "reduce_output_format": "dataframe"
  },
  "parser": {
    "parse_type": "LinkAlign",
    "is_save_parse": true,
    "parse_save_dir": "files/schema_links",
    "parse_output_format": "dataframe"
  },
  "generator": {
    "generate_type": "LinkAlign",
    "is_save_generate": true,
    "generate_save_dir": "files/pred_sql"
  },
  "task": {
    "task_meta": [
      {
        "task_id": "spider_dev_generate",
        "task_name": "Spider Dev SQL Generation",
        "task_info": "Generate SQL queries for Spider dev dataset using LinkAlign method",
        "task_type": "generate",
        "data_source": "spider:dev",
        "schema_source": "spider:dev",
        "eval_type": ["exact_match", "execution"],
        "log_save_path": "files/logs/spider_dev_generate.log",
        "is_save_dataset": true,
        "dataset_save_path": "files/datasets/spider_dev_processed.json",
        "open_parallel": true,
        "max_workers": 5,
        "meta": {
          "dataset": {
            "random_size": null,
            "filter_by": null,
            "multi_database": false,
            "vector_store": "vector_store",
            "embed_model_name": "BAAI/bge-large-en-v1.5",
            "db_credential": {},
            "db_path": "benchmarks/spider/database",
            "is_schema_final": false
          },
          "llm": {
            "use": "qwen",
            "model_name": "qwen-turbo",
            "context_window": 120000,
            "max_token": 8000,
            "top_p": 0.9,
            "temperature": 0.75,
            "time_out": 300.0
          },
          "task": {
            "save_dir": "files/pred_sql",
            "is_save": true,
            "use_external": true,
            "use_few_shot": true,
            "use_feedback_debug": true,
            "debug_turn_n": 2
          },
          "actor": {
            "is_save": true,
            "save_dir": "files/pred_sql",
            "use_external": true,
            "use_few_shot": true,
            "use_feedback_debug": true,
            "debug_turn_n": 2
          }
        }
      }
    ],
    "cpx_task_meta": [
      {
        "task_id": "spider_dev_pipeline",
        "task_name": "Spider Dev Complete Pipeline",
        "task_info": "Complete Text-to-SQL pipeline for Spider dev dataset: Reduce -> Parse -> Generate",
        "task_lis": ["spider_dev_reduce", "spider_dev_parse", "spider_dev_generate"],
        "eval_type": ["exact_match", "execution"],
        "log_save_path": "files/logs/spider_dev_pipeline.log",
        "is_save_dataset": true,
        "dataset_save_path": "files/datasets/spider_dev_pipeline.json",
        "open_parallel": true,
        "max_workers": 5,
        "open_actor_parallel": true,
        "meta": {
          "task": {
            "save_dir": "files/pipeline_output",
            "is_save": true
          },
          "actor": {
            "spider_dev_reduce": {
              "reduce_type": "LinkAlign",
              "is_save": true,
              "save_dir": "files/instance_schemas"
            },
            "spider_dev_parse": {
              "parse_type": "LinkAlign",
              "is_save": true,
              "save_dir": "files/schema_links"
            },
            "spider_dev_generate": {
              "generate_type": "LinkAlign",
              "is_save": true,
              "save_dir": "files/pred_sql",
              "use_external": true,
              "use_few_shot": true,
              "use_feedback_debug": true,
              "debug_turn_n": 2
            }
          }
        }
      }
    ],
    "is_save_dataset": true,
    "open_parallel": true,
    "max_workers": 5
  },
  "engine": {
    "exec_process": ["spider_dev_generate"]
  },
  "credential": {
    "big_query": "path/to/big_query_credential.json",
    "snowflake": "path/to/snowflake_credential.json"
  }
} 